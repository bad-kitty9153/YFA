{
	"name": "Youtube Finished At",
	"version": "1.0",
	"description": "Shows the time when you will finish a Youtube video!",
	"permissions": [
		"https://*.youtube.com/*"
	],
	"content_scripts": [
		{
			"matches": ["https://*.youtube.com/*"],
			"js": ["content.js"],
			"run_at": "document_start"
		}
	],
	"icons": {
		"16": "Icons/16w/YFA Icon.png",
		"48": "Icons/48w/YFA Icon.png",
		"128": "Icons/128w/YFA Icon.png"
	},
	"manifest_version": 2
}